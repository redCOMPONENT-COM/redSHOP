<div class="form-check form-check-inline">
    <input class="form-check-input" type="radio"
           id="shipping_rate_id_{{ shippingMethod.extension_id }}_{{ index }}" name="shipping_rate_id"
           value="{{ rate.value }}" {{ checked }}
           onclick="javascript:onestepCheckoutProcess(this.name,'{{ className }}');"/>
    <label class="form-check-label" for="shipping_rate_id_{{ shippingMethod.extension_id }}_{{ index }}">
       {{ rateText }}
    </label>
</div>