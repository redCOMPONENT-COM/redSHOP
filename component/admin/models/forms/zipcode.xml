<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="details">
		<field
			name="id"
			type="hidden"
			/>
		<field
			id="country_code"
			name="country_code"
			type="rcountry3code"
			label="COM_REDSHOP_COUNTRY_NAME"
			description="COM_REDSHOP_COUNTRY_NAME"
			size="30"
			class="text_area"
			default=""
			required="true"
			onchange="getState2Code();"
			/>
		<field
			id="state_code"
			name="state_code"
			type="rstate2code"
			label="COM_REDSHOP_STATE_NAME"
			description="COM_REDSHOP_STATE_NAME"
			size="30"
			class="text_area"
			default=""
			required="false"
			/>
		<field
			id="city_name"
			name="city_name"
			type="text"
			label="COM_REDSHOP_CITY_NAME"
			description="COM_REDSHOP_CITY_NAME"
			size="30"
			class="text_area"
			default=""
			required="true"
			/>
		<field
			id="zipcode"
			name="zipcode"
			type="text"
			label="COM_REDSHOP_FROM"
			description="COM_REDSHOP_FROM"
			size="80"
			class="text_area"
			default=""
			required="true"
			/>
		<field
			id="zipcodeto"
			name="zipcodeto"
			type="text"
			label="COM_REDSHOP_TO"
			description="COM_REDSHOP_TO"
			size="80"
			class="text_area"
			default=""
			required="false"
			/>
	</fieldset>

</form>
