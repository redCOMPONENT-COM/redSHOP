<?xml version="1.0" encoding="utf-8"?><extension type="module" version="2.5" client="site" method="upgrade">    <name>Redshop Products Slide Show</name>    <version>1.3</version>    <creationDate>April 2013</creationDate>    <author>redCOMPONENT.com</author>    <authorEmail>email@redcomponent.com</authorEmail>    <copyright>redCOMPONENT.com</copyright>    <license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>    <description>Redshop Products Slide Show</description>    <files>        <filename module="mod_redshop_products_slideshow">mod_redshop_products_slideshow.php</filename>        <filename module="mod_redshop_products_slideshow">index.html</filename>        <filename module="mod_redshop_products_slideshow">slideshow.swf</filename>        <filename module="mod_redshop_products_slideshow">AC_RunActiveContent.js</filename>    </files>    <config>        <fields name="params">            <fieldset name="basic">                <field name="moduleclass_sfx" type="text" default="" label="Module Class Suffix"                       description="A suffix to be applied to the css class of the module (table.moduletable), this allows individual module styling."/>                <field name="class_sfx" type="text" default="" label="Menu Class Suffix"                       description="A suffix to be applied to the css class of the menu items."/>                <field name="category_id" type="text" default="0" label="Category Id"                       description="Enter comma seperated Category Id's you would like to display products related to a perticular Category. 0 for showing all products and all categories"/>                <field name="load_curr" type="list" default="1"                       label="Load current category images" description="Load current category images">                    <option value="1">yes</option>                    <option value="2">no</option>                </field>                <field name="loadtype" type="list" default="random"                       label="Product load by" description="Product loading type">                    <option value="">Select</option>                    <option value="newest">Newest Product</option>                    <option value="random">Random Products</option>                    <option value="mostsold">Most sold Products</option>                    <option value="special">Special Products</option>                </field>                <field name="bannerWidth" type="text" default="670"                       label="Gallery Width (px)" description="Width of object ."/>                <field name="bannerHeight" type="text" default="210"                       label="Gallery Height (px)" description="Height of object "/>                <field name="backgroundColor" type="text" default="#ffffff"                       label="Object Background color"                       description="For example: #ffffff. Slect wmode transperant if you dont want background color"/>                <field name="imageWidth" type="text" default="200" label="Image Width (px)"                       description="Width of Image ."/>                <field name="imageHeight" type="text" default="100"                       label="Image Height (px)" description="Height of Image "/>                <field name="numbproduct" type="text" default="5"                       label="Number of Products" description="Number of Products display"/>                <field name="wmode" type="list" default="window" label="wmode"                       description="Select the wmode of flash">                    <option value="opaque">opaque</option>                    <option value="transparent">transparent</option>                    <option value="window">window</option>                </field>                <field name="roundCorner" type="text" default="0" label="Round Corner"                       description="0,1,2,3...,10(most often used),11..."/>                <field name="autoPlayTime" type="text" default="8" label="Auto Play Time"                       description="Auto Play Time"/>                <field name="isHeightQuality" type="list" default="no"                       label="Height Quality" description="Height Quality">                    <option value="yes">yes</option>                    <option value="no">no</option>                </field>                <field name="blendMode" type="text" default="normal" label="Blend Mode"                       description="Round Corner"/>                <field name="transDuration" type="text" default="1"                       label="Transition Duration" description="Transition Duration"/>                <field name="windowOpen" type="list" default="_blank" label="Target"                       description="Window Open Target fieldeter">                    <option value="_self">_self</option>                    <option value="_blank">_blank</option>                    <option value="_parent">_parent</option>                </field>                <field name="btnSetMargin" type="text" default="auto 5 5 auto"                       label="Button Set Margin" description="Button Set Margin"/>                <field name="btnDistance" type="text" default="20" label="Button Distance"                       description="Button Distance"/>                <field name="titleBgColor" type="text" default="0x000000"                       label="Title Background Color" description="Title Background Color"/>                <field name="titleTextColor" type="text" default="0xffffff"                       label="Title Text Color" description="Title Text Color"/>                <field name="titleBgAlpha" type="text" default=".4"                       label="Title Background Alpha" description="Title Background Alpha"/>                <field name="titleMoveDuration" type="text" default="1"                       label="Title Move Duration" description="Title Move Duration"/>                <field name="btnAlpha" type="text" default=".4" label="Button Alpha"                       description="Button Alpha"/>                <field name="btnTextColor" type="text" default="0xffffff"                       label="Button Text Color" description="Button Text Color"/>                <field name="btnDefaultColor" type="text" default="0x1B3433"                       label="Button Default Color" description="Button Default Color"/>                <field name="btnHoverColor" type="text" default="0xff9900"                       label="Button Hover Color" description="Button Hover Color"/>                <field name="btnFocusColor" type="text" default="0xff6600"                       label="Button Focus Color" description="Button Focus Color"/>                <field name="changImageMode" type="list" default="click"                       label="Change Image Mode" description="Change Image Mode">                    <option value="click">click</option>                    <option value="hover">hover</option>                </field>                <field name="isShowBtn" type="list" default="yes" label="Show Button"                       description="Show Button">                    <option value="yes">yes</option>                    <option value="no">no</option>                </field>                <field name="isShowTitle" type="list" default="yes" label="Show Title"                       description="Show Title">                    <option value="yes">yes</option>                    <option value="no">no</option>                </field>                <field name="scaleMode" type="list" default="noScale" label="Scale Mode"                       description="Scale Mode">                    <option value="showAll">showAll</option>                    <option value="exactFit">exactFit</option>                    <option value="noScale">noScale</option>                    <option value="noBorder">noBorder</option>                </field>                <field name="transform" type="list" default="blur" label="Transform"                       description="Transform">                    <option value="blur">blur</option>                    <option value="alpha">alpha</option>                    <option value="left">left</option>                    <option value="right">right</option>                    <option value="top">top</option>                    <option value="bottom">bottom</option>                    <option value="breathe">breathe</option>                    <option value="breathBlur">breathBlur</option>                </field>                <field name="isShowAbout" type="list" default="no" label="Show About"                       description="Show About">                    <option value="yes">yes</option>                    <option value="no">no</option>                </field>                <field name="titleFont" type="text" default="Tahoma" label="Title Font"                       description="Title Font"/>                <field name="show_price" type="list" default="yes" label="Show Price"                       description="Select yes or no to show or hide price">                    <option value="yes">yes</option>                    <option value="no">no</option>                </field>                <field name="currency" type="text" default="$" label="Currency Symbol"                       description="Currency Symbol"/>                <field name="pricetax" type="list" default="yes"                       label="Display prices with tax" description="Display prices with tax">                    <option value="yes">yes</option>                    <option value="no">no</option>                </field>            </fieldset>        </fields>    </config></extension>